{
  "version": 2,
  "name": "now-node-fs",
  "public": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
    {
      "src": "*.js",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "/.*",
      "dest": "index.js"
    }
  ]
}
